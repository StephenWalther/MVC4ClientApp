
<h1>Billing History</h1>

<table>
<thead>
    <tr>
        <th>Id</th>
        <th>Amount</th>
        <th>Date</th>
    </tr>
</thead>
<tbody data-bind="foreach:billingHistory">
    <tr>
        <td data-bind="text:Id"></td>
        <td data-bind="text:Amount"></td>
        <td data-bind="dateString: DateCreated, datePattern: 'dddd, MMMM dd, yyyy'"></td>
    </tr>
</tbody>
</table>

@section scripts {
    <script>    
        ko.applyBindings(new MyApp.ViewModels.HomeViewModel());
    </script>
}


